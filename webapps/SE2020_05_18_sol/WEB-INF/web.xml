<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

    <servlet>
        <servlet-name>EmployeesList</servlet-name>
        <servlet-class>EmployeesList</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>EmployeesListDB</servlet-name>
        <servlet-class>EmployeesListDB</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>UpdateExtension</servlet-name>
        <servlet-class>UpdateExtension</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>InsertEmployee</servlet-name>
        <servlet-class>InsertEmployee</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>EmployeesList</servlet-name>
        <url-pattern>/EmployeesList</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>EmployeesListDB</servlet-name>
        <url-pattern>/EmployeesListDB</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpdateExtension</servlet-name>
        <url-pattern>/UpdateExtension</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertEmployee</servlet-name>
        <url-pattern>/InsertEmployee</url-pattern>
    </servlet-mapping>

</web-app>