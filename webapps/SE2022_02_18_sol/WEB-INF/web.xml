<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app>

    <servlet>
        <servlet-name>ProductListMockup</servlet-name>
        <servlet-class>ProductListMockup</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>ProductCards</servlet-name>
        <servlet-class>ProductCards</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ProductListMockup</servlet-name>
        <url-pattern>/ProductListMockup</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>ProductCards</servlet-name>
        <url-pattern>/ProductCards</url-pattern>
    </servlet-mapping>

</web-app>
